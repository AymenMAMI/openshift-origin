{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"_artifactsLocation": {
			"value": "https://raw.githubusercontent.com/AymenMAMI/openshift-origin/azurestack-release-3.9"
		},
		"masterVmSize": {
			"value": "Standard_DS2_v2"
		},
		"infraVmSize": {
			"value": "Standard_DS2_v2"
		},
		"nodeVmSize": {
			"value": "Standard_DS2_v2"
		},
		"storageKind": {
			"value": "unmanaged"
		},
		"openshiftClusterPrefix": {
			"value": "RFC"
		},
		"masterInstanceCount": {
			"value": 1
		},
		"infraInstanceCount": {
			"value": 1
		},
		"nodeInstanceCount": {
			"value": 1
		},
		"dataDiskSize": {
			"value": 128
		},
		"adminUsername": {
			"value": "openshiftuser"
		},
		"openshiftPassword": {
			"value": "OpenSHIFT#2020"
		},
		"enableMetrics": {
			"value": "true"
		},
		"enableLogging": {
			"value": "true"
		},
		"sshPublicKey": {
			"value": ""
		},
		"keyVaultResourceGroup": {
			"value": "OKD-KeyVault"
		},
		"keyVaultName": {
			"value": "okdkeyvault"
		},
		"keyVaultSecret": {
			"value": "sshprivatekey"
		},
		"enableAzure": {
			"value": "false"
		},
		"defaultSubDomainType": {
			"value": "custom"
		},
		"defaultSubDomain": {
			"value": "xpressazure.tn"
		}
	}
}
